{
    "presets": [
      ["@babel/preset-env", {"modules": false}],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-proposal-optional-chaining"
    ],
    "env": {
      "production": {
        "presets": [["minify", { "builtIns": false}]]
      },
      "test": {
        "presets": ["@babel/preset-env", "@babel/preset-react"]
      }
    }
}